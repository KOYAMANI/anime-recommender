{
    "family": "frontend-task",
    "executionRoleArn": "<EXECUTION_ROLE_ARN>",
    "taskRoleArn": "<TASK_ROLE_ARN>",
    "networkMode": "awsvpc",
    "containerDefinitions": [
      {
        "name": "frontend-task",
        "image": "<IMAGE>",
        "portMappings": [
          {
            "containerPort": 80,
            "protocol": "tcp"
          }
        ],
        "essential": true,
        "secrets": [
          {
            "name": "REACT_APP_API_URL",
            "valueFrom": "arn:aws:secretsmanager:eu-central-1:588729807438:secret:anime-recommender/frontend-6XHGXD"
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "anime-recommender-frontend-log",
            "awslogs-region": "eu-central-1",
            "awslogs-stream-prefix": "frontend-task"
          }
        }
      }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "512",
    "memory": "2GB"
  }
  